
.wrap {
	width: 90%;
	margin: 0 auto;
}
.head {
	padding-top: 20px;
	height: 110rpx;
	/* border: 1upx solid red; */
	background-color: black;
}
.head ._p {
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	color: white;
	/* border: 1upx solid red; */
}
.head .info {
	height: 75rpx;
	/* border: 1upx solid red; */
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
}
.head .info .left {
	width: 69rpx;
	height: 69rpx;
	/* border: 1upx solid white; */
	border-radius: 50%;
	background-image: url(data:image/png;base64,);
	background-position: 0.35px 0;
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.head .info .right {
	/* border: 1upx solid white; */
	width: 500rpx;
	margin-left: 40rpx;
}
.head .info .right .top {
	margin-top: 6rpx;
	height: 30rpx;
	font-size: 28rpx;
	line-height: 30rpx;
	text-align: left;
}
.head .info .right .bottom {
	height: 25rpx;
	font-size: 24rpx;
	line-height: 25rpx;
	text-align: left;
	color: #808080;
	/* border: 1px solid red; */
	margin-top: 10rpx;
}
.body{
	height: 1000rpx;
	/* border: 1px solid black; */
}
.body .card {
	/* border: 1px solid black; */
	height: 160rpx;
	width: 100%;
	margin-top: 50rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	background: -webkit-gradient(linear, left top, right bottom, from(#FCE5CF), to(#E6BB98));
	background: -o-linear-gradient(top left, #FCE5CF, #E6BB98);
	background: linear-gradient(to bottom right, #FCE5CF, #E6BB98);
	border-radius: 10rpx;
}
.body .card .left {
	/* border: 1px solid black; */
	height: 160rpx;
	width: 70%;
}
.body .card .left .top {
	color: black;
	margin-left: 25rpx;
	margin-top: 30rpx;
	height: 50rpx;
	line-height: 50rpx;
	/* border: 1px solid black; */
	font-size: 40rpx;
}
.body .card .left .bottom {
	font-size: 20rpx;
	margin-left: 25rpx;
	height: 50rpx;
	line-height: 50rpx;
	/* border: 1px solid black; */
	color: #808080;
}
.body .card .right {
	/* border: 1px solid black; */
	height: 160rpx;
	width: 30%;
}
.body .card .right .single {
	height: 50rpx;
	margin-top: 45rpx;
	text-align: end;
	margin-right: 30rpx;
	font-weight: bold;
	font-size: 45rpx;
}
.body .word .f1 {
	margin-top: 30rpx;
	border-top: 1px solid #e0e0e0;
	border-bottom: 1px solid #e0e0e0;
	height: 100rpx;
	line-height: 100rpx;
}
.body .word .f1 .wrap {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.body .word .f1 text:last-child {
	/* border: 1px solid red; */
	font-size: 30rpx;
	color: #808080;
}
.body .word .f1 text:first-child {
	/* border: 1px solid red; */
	font-size: 35rpx;
}
.body .word .f2 {
	border-bottom: 1px solid #e0e0e0;
	height: 100rpx;
	line-height: 100rpx;
}
.body .word .f2 .wrap {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.body .word .f2 text:last-child {
	/* border: 1px solid red; */
	font-size: 30rpx;
	color: #808080;
}
.body .word .f2 text:first-child {
	/* border: 1px solid red; */
	font-size: 35rpx;
}
.body .word .f3 {
	height: 100rpx;
	line-height: 100rpx;
	color: #808080;
	font-size: 28rpx;
}
.body .word .f3 .wrap {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.body .word .f3 text:first-child {
	/* border: 1px solid pink; */
	padding: 0 10rpx;
	margin-right: 10rpx;
}
.body .word .f3 text:first-child:after {
	content: '';
	display: inline-block;
	position: relative;
	width: 1px;
	height: 28rpx;
	background-color: #808080;
	top: 5rpx;
	right: -11rpx;
}
.foot{
	/* border: 1px solid black; */
	margin-top: 20rpx;
	border-radius: 50rpx;
}
.foot navigator{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.foot navigator .left{
	height: 95rpx;
	width: 65%;
	border-radius: 50rpx 0 0 50rpx;
	background-color: black;
	line-height: 95rpx;
}
.foot navigator .left text:first-child{
	color: #808080;
	margin-left: 40rpx;
	font-size: 25rpx;
}
.foot navigator .left text:last-child{
	color: white;
}
.foot navigator .right{
	height: 95rpx;
	width: 35%;
	background-color: #E6BB98;
	border-radius: 0 50rpx 50rpx 0;
	line-height: 95rpx;
	text-align: center;
	font-size: 30rpx;
}

