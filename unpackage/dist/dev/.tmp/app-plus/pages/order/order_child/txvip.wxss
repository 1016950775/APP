.box{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	font-size: 35rpx;
	margin-bottom: 20rpx;
}
.btn{
	height: 80rpx;
	width: 50%;
	margin: 10rpx 0 0 -20rpx;
	border-bottom-left-radius: 60rpx;
	border-top-left-radius: 60rpx;
	color: #ECEAE6;
	background-color: #332D2D;
	text-align: left;
	line-height: 80rpx;
	font-weight: bold;
	font-size: 28rpx;
	padding-left: 50rpx;
}
.btn2{
	height: 80rpx;
	width: 30%;
	margin: 10rpx 0 0 -3rpx;
	border-bottom-right-radius: 60rpx;
	border-top-right-radius: 60rpx;
	background: -webkit-gradient(linear, left top, right top, from(#FAE8D9) , to(#E4C5A9));
	background: -o-linear-gradient(left, #FAE8D9 , #E4C5A9);
	background: linear-gradient(to right, #FAE8D9 , #E4C5A9);
	text-align: center;
	line-height: 80rpx;
	font-weight: bold;
	color: #563726;
	font-size: 28rpx;
}

